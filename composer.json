{
  "name": "drupal/behat_ui",
  "description": "Create and run Behat/Mink tests from the web interface",
  "type": "drupal-module",
  "homepage": "https://www.drupal.org/project/behat_ui",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Webship",
      "homepage": "http://webship.org",
      "role": "Maintainer"
    }
  ],
  "support": {
    "issues": "https://www.drupal.org/project/issues/behat_ui",
    "source": "http://cgit.drupalcode.org/behat_ui"
  },
  "require": {
    "composer/installers": "~1.0",
    "oomphinc/composer-installers-extender": "~1.0",
    "cweagans/composer-patches": "~1.0",
    "drupal/drupal-library-installer-plugin": "^0.3"
  },
  "extra": {
    "branch-alias": {
      "dev-8.x-3.x": "8.3.x-dev"
    },
    "enable-patching": true,
    "composer-exit-on-patch-failure": true
  }
}
